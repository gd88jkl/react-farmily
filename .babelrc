{
  "presets": [
    ["@babel/preset-env",{
      "modules": false,
      "useBuiltIns": "entry"
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators", 
      { "legacy": true }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    // [
    //   "@babel/plugin-transform-runtime",
    //   {
    //     "corejs": false,
    //     "helpers": true,
    //     "regenerator": true,
    //     "useESModules": false
    //   }
    // ]
  ]
}